<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$PackageId$</id>
    <version>$NuGetPackageVersion$</version>
    <authors>$Authors$</authors>
    <owners>$Authors$</owners>
    <requireLicenseAcceptance>$PackageRequireLicenseAcceptance$</requireLicenseAcceptance>
    <licenseUrl>$PackageLicenseUrl$</licenseUrl>
    <projectUrl>$PackageProjectUrl$</projectUrl>
    <description>$PackageDescription$</description>
    <copyright>$Copyright$</copyright>
    <tags>$PackageTags$</tags>
    <repository url="$RepositoryUrl$" />
    <dependencies>
      <group targetFramework=".NETFramework4.6">
        <dependency id="Microsoft.Build" version="15.7.179" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.Build" version="15.7.179" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$Configuration$\net46\$AssemblyName$.dll" target="lib\net46\$AssemblyName$.dll" />
    <file src="bin\$Configuration$\net46\$AssemblyName$.xml" target="lib\net46\$AssemblyName$.xml" />
    <file src="bin\$Configuration$\netstandard2.0\$AssemblyName$.dll" target="lib\netstandard2.0\$AssemblyName$.dll" />
    <file src="bin\$Configuration$\netstandard2.0\$AssemblyName$.xml" target="lib\netstandard2.0\$AssemblyName$.xml" />
  </files>
</package>